<h2 id="Webforms">Webforms</h2>

   <input type="text" placeholder="This is a text field!">
            
 
  <form action="url-where-you-want-to-submit-form-data">
 <input type="text" placeholder="This is a text field!"></form>
  
    <form action="url-where-you-want-to-submit-form-data">
 <input type="text" required placeholder="This is a text field with the required attribute"></form>

  
  
  <button type="submit">Click here to submit the form!</button>
  
 
 
